	</head>
<body>
<!--banner-->
<div class="banner-top">
	<div class="container">
		<h2 class="animated wow fadeInLeft" data-wow-delay=".5s">注册</h2>
		<h3 class="animated wow fadeInRight" data-wow-delay=".5s"><a href="">主页</a><label>/</label>注册</h3>
		<div class="clearfix"> </div>
	</div>
</div>
<!-- contact -->
	<div class="login">
		<div class="container">
		<form>
			<div class="col-md-3"></div>
			<div class="col-md-6 login-do1 animated wow fadeInLeft" data-wow-delay=".5s">
				<div class="login-mail">
					<i class="glyphicon glyphicon-user"></i>
					<input type="text" placeholder="用户名" required="">
				</div>
				<div class="login-mail">
					<i class="glyphicon glyphicon-lock"></i>
					<input type="password" placeholder="密码" required="">
				</div>
				<div class="login-mail">
					<i class="glyphicon glyphicon-lock"></i>
					<input type="password" placeholder="确认密码" required="">
				</div>
				<div class="login-mail">
					<i class="glyphicon glyphicon-earphone"></i>
					<input type="text" placeholder="电话" required="">
				</div>
				  <a class="news-letter" href="#">
					 <label class="checkbox1"><input type="checkbox" name="checkbox" ><i> </i>我同意以下条款</label>
				  </a>
			</div>
			<div class="col-md-6"></div>
			<div class="col-md-3 login-do animated wow fadeInRight" data-wow-delay=".5s">
				<label class="hvr-sweep-to-top login-sub">
					<input type="submit" value="提交">
					</label>
					<p>已经注册？</p>
				<a href="login.html" class="hvr-sweep-to-top">登录</a>
			</div>
			<div class="clearfix"> </div>
			</form>
		</div>


	</div>

</body>
<!-- js -->
<script src="<%=basePath %>/styles/insurance/js/jquery.min.js"></script>
